ne\" class=\"form-check-input\"> -->\n  </div>\n  </div>\n  <div class =\"row bg-secondary py-2 mt-2 text-white\">\n<div class=\"col text-center\">\n  <input type=\"check-box\" v-model=\"hideCompleted\" class=\"form-check-input\"/>\n  <label class=\"form-check-label font-weight-bold\">\nHide Completed Tasks\n    </label>\n</div>\n</div>\n<div class=\"form-group m-2\">\n            <label>New Item:</label>\n            <input v-model=\"newItemText\" class=\"form-control\" />\n        </div>\n        <div class=\"text-center\">\n            <button class=\"btn btn-primary\" v-on:click=\"addNewTodo\">\n                Add\n            </button>\n        </div>\n</div>\n    \n</template>\n\n<script>\n//import HelloWorld from './components/HelloWorld.vue'\n\nexport default {\n  name: 'app',\n\n  data(){\n\n    return{\n\n      name:\"Pizza\",\n\n      tasks:[{action: \"Buy Flowers\",done:false},\n              { action: \"Get Shoes\",done:false},\n              { action: \"Collect Tickets\",done:true},\n\n               {action: \"Call Sumaya\",done:true},\n              \n      \n      ],\n      hideCompleted: true\n    }\n  },\n  computed:{\n   filteredTasks(){\nreturn this.hideCompleted ?\nthis.tasks.filter(t => !t.done) : this.tasks\n\n   }\n\n  }\n\n    \n  }\n\n</script>\n\n<style>\n#app {\n  font-family: Avenir, Helvetica, Arial, sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-align: center;\n\n  color: #2c3e50;\n  margin-top: 60px;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}